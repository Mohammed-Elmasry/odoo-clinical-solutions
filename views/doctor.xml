<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="view_all_employees_form" model="ir.ui.view">
        <field name="name">my all employees</field>
        <field name="model">doctor.info.model</field>
        <field name="arch" type="xml">
            <form>
                <group string="yallahwy" col="4">
                    <field name="description"/>
                    <field name="name"/>
                </group>
            </form>
        </field>
    </record>

    <record model="ir.actions.act_window" id="action_all_employees_tree">
        <field name="name">clinical_management_system window</field>
        <field name="res_model">doctor.info.model</field>
        <field name="view_mode">form</field>
    </record>

    <act_window id="action_all_employees"
                name="All employees"
                res_model="doctor.info.model"
                view_mode="tree,form"/>

    <menuitem name="all employees" id="clinical_management_system.menu_1_list" parent="clinical_management_system.menu_root"
              action="action_all_employees"/>

    <menuitem id="menu_custom_toplevel" name="my custom menu" />
    <menuitem id="menu_custom_left" parent="menu_custom_toplevel"
              name="this will appear in the left part"/>
    <menuitem id="menu_all_employees" parent="menu_custom_left" action="action_all_employees"
              sequence="10"/>
</odoo>